[
  {
    "domain": "Public Transportation Feedback",
    "mapping": {
      "properties": {
        "feedback_id": { "type": "keyword" },
        "user_id": { "type": "keyword" },
        "route_name": { "type": "text" },
        "submission_date": { "type": "date" },
        "rating": { "type": "integer" },
        "comments": { "type": "text" }
      }
    },
    "NLQ": "Retrieve all feedback with a rating of 5 submitted this year.",
    "query": {
      "query": {
        "bool": {
          "must": { "term": { "rating": 5 } },
          "filter": { "range": { "submission_date": { "gte": "2023-01-01" } } }
        }
      }
    }
  },
  {
    "domain": "Content Management System Logs",
    "mapping": {
      "properties": {
        "log_id": { "type": "keyword" },
        "timestamp": { "type": "date" },
        "user_action": { "type": "text" },
        "user_id": { "type": "keyword" },
        "content_id": { "type": "keyword" },
        "changes": { "type": "text" }
      }
    },
    "NLQ": "Find logs of content deletions in the last month.",
    "query": {
      "query": {
        "bool": {
          "must": { "match": { "user_action": "delete" } },
          "filter": { "range": { "timestamp": { "gte": "now-1M/M" } } }
        }
      }
    }
  },
  {
    "domain": "Digital Library Access Records",
    "mapping": {
      "properties": {
        "access_id": { "type": "keyword" },
        "user_id": { "type": "keyword" },
        "document_id": { "type": "keyword" },
        "access_time": { "type": "date" },
        "duration_seconds": { "type": "integer" }
      }
    },
    "NLQ": "Show access records longer than 10 minutes for document 123 in the past week.",
    "query": {
      "query": {
        "bool": {
          "must": { "term": { "document_id": "123" } },
          "filter": [
            { "range": { "access_time": { "gte": "now-7d/d" } } },
            { "script": { "script": { "source": "doc['duration_seconds'].value > 600", "lang": "painless" } } }
          ]
        }
      }
    }
  }
]
