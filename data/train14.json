{
    "domain": "Real Estate Listings",
    "mapping": {
      "properties": {
        "listing_id": { "type": "keyword" },
        "address": { "type": "text" },
        "city": { "type": "keyword" },
        "price": { "type": "double" },
        "bedrooms": { "type": "integer" },
        "bathrooms": { "type": "integer" },
        "square_feet": { "type": "integer" },
        "listing_date": { "type": "date" },
        "features": { "type": "keyword" }
      }
    },
    "NLQs": [
      {
        "NLQ": "Find listings with at least 3 bedrooms and 2 bathrooms in 'Seattle'.",
        "query": {
          "query": {
            "bool": {
              "must": [
                { "term": { "city": "Seattle" } },
                { "range": { "bedrooms": { "gte": 3 } } },
                { "range": { "bathrooms": { "gte": 2 } } }
              ]
            }
          }
        }
      },
      {
        "NLQ": "Show recent listings under $500,000 with a patio.",
        "query": {
          "query": {
            "bool": {
              "must": { "term": { "features": "patio" } },
              "filter": [
                { "range": { "price": { "lte": 500000 } } },
                { "range": { "listing_date": { "gte": "now-3M/M" } } }
              ]
            }
          }
        }
      }
    ]
  }
  