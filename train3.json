{
  "domain": "Online Course Catalog",
  "mapping": {
    "course_id": { "type": "keyword" },
    "title": { "type": "text" },
    "instructor": { "type": "text" },
    "category": { "type": "keyword" },
    "price": { "type": "double" },
    "rating": { "type": "double" },
    "enrollment_date": { "type": "date" },
    "language": { "type": "keyword" },
    "duration_hours": { "type": "double" }
  },
  "NLQs": [
    {
      "NLQ": "Show free programming courses in Spanish.",
      "query": {
        "query": {
          "bool": {
            "must": [
              { "term": { "category": "programming" } },
              { "term": { "language": "Spanish" } }
            ],
            "filter": { "term": { "price": 0 } }
          }
        }
      }
    },
    {
      "NLQ": "Find all courses by Dr. Smith with a rating above 4.5.",
      "query": {
        "query": {
          "bool": {
            "must": { "match": { "instructor": "Dr. Smith" } },
            "filter": { "range": { "rating": { "gt": 4.5 } } }
          }
        }
      }
    },
    {
      "NLQ": "What are the longest courses available in the category of data science?",
      "query": {
        "query": {
          "bool": {
            "must": { "term": { "category": "data science" } }
          }
        },
        "sort": [{ "duration_hours": { "order": "desc" } }]
      }
    },
    {
      "NLQ": "List all business courses that started enrollment this year.",
      "query": {
        "query": {
          "bool": {
            "must": { "term": { "category": "business" } },
            "filter": { "range": { "enrollment_date": { "gte": "2023-01-01" } } }
          }
        }
      }
    },
    {
      "NLQ": "Which courses cost between $50 and $100 and are available in English?",
      "query": {
        "query": {
          "bool": {
            "must": { "term": { "language": "English" } },
            "filter": {
              "range": {
                "price": {
                  "gte": 50,
                  "lte": 100
                }
              }
            }
          }
        }
      }
    }
  ]
}
